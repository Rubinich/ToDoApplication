<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="hr.projekt.todoapplication.controller.EventCardController">
   <padding>
      <Insets bottom="5.0" left="15.0" right="15.0" top="5.0"/>
   </padding>
   <VBox fx:id="cardContainer" prefHeight="100.0" spacing="35"
         style="-fx-padding: 10;       -fx-border-color: grey;       -fx-border-width: 3;       -fx-border-radius: 8;       -fx-background-radius: 8; "
         AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
      <Label fx:id="titleLabel" maxWidth="150" prefWidth="1024.0" style="-fx-font-weight: bold;             "
             text="Label"/>
      <Label fx:id="dateLabel" prefWidth="1024.0" text="Label"/>
   </VBox>
</AnchorPane>
